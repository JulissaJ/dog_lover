<h1>Dog Owners</h1>

<table>
  <thead>
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>E-Mail</th>
      <th>Dog's name</th>
      </tr>
  </thead>

  <tbody>
    <% @dog_owners.each do |dog_owner| %>
      <tr>
        <td><%= dog_owner.first_name %></td>
        <td><%= dog_owner.last_name %></td>
        <td><%= dog_owner.email %></td>
        <td><%= dog_owner.dog_name %></td>
       <% end %>
  </tbody>
</table>

<%= link_to 'Add new dog owner', new_dog_owner_path %>


